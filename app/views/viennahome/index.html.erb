        <div class="carousel-inner" role="listbox">

          <!-- Slide 1 -->
          <div class="carousel-item active" style='background: linear-gradient(90deg, rgba(0, 0, 0, 0.2) 0%, rgba(0, 0, 0, 0.2) 100%), url(<%= asset_path 'viennahome/fontvienna.jpg' %>)center no-repeat; background-size: cover; background-position: center;background-repeat: no-repeat;' >
            <div class="carousel-container">
              <div class="carousel-content">
                <h2 class="animate__animated animate__fadeInDown">Vienna Home</h2>
                <p class="animate__animated animate__fadeInUp">Get furniture product here.</p>
                <div>
                  <a href="#menu" class="btn-menu btn btn-secondary animate__animated animate__fadeInUp scrollto">Our Product</a>
                </div>
              </div>
            </div>
          </div>

          <!-- Slide 2 -->

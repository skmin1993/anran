<div class="row mt20 mb20">
  <%= render 'header' %>
</div>

  <!-- ======= Hero Section ======= -->
  <section id="product-hero" class="clearfix">
    <div class="container d-flex h-100">
      <div class="row justify-content-center align-self-center" data-aos="fade-up">
        <div class="col-md-6 intro-info order-md-first order-last" data-aos="zoom-in" data-aos-delay="100">
          <h2>Cage Fabrication</h2>
          <h4><span></span></h4>
          <div>
          </div>
        </div>

        <div class="col-md-6 intro-img order-md-last order-first" data-aos="zoom-out" data-aos-delay="200">
          <img src="assets/img/intro-img.svg" alt="" class="img-fluid">
        </div>
      </div>

    </div>
  </section><!-- End Hero -->

<br>
  <main id="main">

    <!-- ======= Works Section ======= -->
    <section class="section site-portfolio">
      <div class="container">
        <div class="row mb-5 align-items-center">
          <div class="col-md-12 col-lg-6 mb-4 mb-lg-0" data-aos="fade-up">
            <h2>Our Portfolio</h2>
            <p class="mb-0"></p>
          </div>
        </div>
        <div id="portfolio-grid" class="row no-gutter" data-aos="fade-up" data-aos-delay="200">
          <div class="item web col-sm-6 col-md-4 col-lg-4 mb-4">
            <a href="<%= asset_path('flyingengineering/cage1.jpg')%>" class="item-wrap venobox">
              <div class="work-info">
                <h3>Cage Fabrication 1</h3>
              </div>
              <img class="img-fluid" src="<%= asset_path('flyingengineering/cage1.jpg')%>" style=" max-height:400px; max-width:470px;">
            </a>
          </div>
          <div class="item photography col-sm-6 col-md-4 col-lg-4 mb-4">
            <a href="<%= asset_path('flyingengineering/cage2.jpg')%>" class="item-wrap venobox">
              <div class="work-info">
                <h3>Caage Fabrication 2</h3>
              </div>
              <img class="img-fluid" src="<%= asset_path('flyingengineering/cage2.jpg')%>" style=" max-height:400px; max-width:470px;">
            </a>
          </div>
        </div>
      </div>
    </section><!-- End  Works Section -->

  


  </main><!-- End #main -->

<div>
  <%= render 'footer' %>
</div>

